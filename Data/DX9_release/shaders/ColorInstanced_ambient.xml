<?xml profile="1.0" encoding="iso-8859-1"?>
<xml>
  <pixelshader name="color3dI">
    <source>ColorInstanced3d.hlsl</source>
    <entry>ps_main</entry>
    <profile>ps_1_1</profile>
	<constants>
	  <matrix name="world" count="1"/>
	  <matrix name="view" count="1"/>
	  <matrix name="projection" count="1"/>
	</constants>
  </pixelshader>
  <vertexshader name="color3dI">
    <source>ColorInstanced3d.hlsl</source>
    <entry>vs_main</entry>
    <profile>vs_1_1</profile>
	<constants>
	  <matrix name="world" count="1"/>
	  <matrix name="view" count="1"/>
	  <matrix name="projection" count="1"/>
	</constants>
	<vertexformat>
      <slot>
        <element name="Position" type="Float3"/>
  	    <element name="Diffuse" type="Color"/>
      </slot>
	</vertexformat>
  </vertexshader>
</xml>